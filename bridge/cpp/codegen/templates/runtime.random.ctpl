#slurp
#compiler-settings
directiveStartToken = %
#end compiler-settings
%slurp

%for $op, $opcode, $optype, $opcount, $typesigs in $data

template <typename T>
multi_array<T>& $op (multi_array<T>& res, uint64_t in1, uint64_t in2)
{
    Runtime::instance().typechecker.check<$opcode, T, uint64_t, uint64_t>();

    Runtime::instance().enqueue((bh_opcode)$opcode, res, in1, in2);

    return res;
}
%end for

