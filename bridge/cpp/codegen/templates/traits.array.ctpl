%slurp
template <typename T>
inline
void assign_array_type( bh_array* array ) {
    // TODO: The general case should result in a meaning-ful compile-time error.
    std::cout << "Unsupported type!" << array << std::cout;
}

%for $ctype, $bh_atype, $bh_ctype, $bh_enum in $data
